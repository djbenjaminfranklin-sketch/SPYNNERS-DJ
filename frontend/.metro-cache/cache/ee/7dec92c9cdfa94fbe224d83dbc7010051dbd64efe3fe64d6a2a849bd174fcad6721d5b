Status();\n        setIsOffline(!isOnline);\n\n        // Register for network changes\n        const unsubscribe = offlineService.default.onNetworkChange(online => {\n          setIsOffline(!online);\n          if (online) {\n            // Network restored - check for pending sessions\n            checkPendingSessions();\n          }\n        });\n\n        // Check for pending sessions\n        await checkPendingSessions();\n        return unsubscribe;\n      } catch (error) {\n        console.error('[SPYN Record] Offline service init error:', error);\n      }\n    };\n\n    // Check for pending sessions to sync\n    const checkPendingSessions = async () => {\n      try {\n        const pendingCount = await offlineService.default.getPendingCount();\n        setPendingSyncCount(pendingCount);\n        if (pendingCount > 0 && !isOffline) {\n          Alert.default.alert('Sessions en attente', `Vous avez ${pendingCount} session(s) Ã  synchroniser. Voulez-vous les synchroniser maintenant ?`, [{\n            text: 'Plus tard',\n            style: 'cancel'\n          }, {\n            text: 'Synchroniser',\n            onPress: syncAllOfflineSessions\n          }]);\n        }\n      } catch (error) {\n        console.error('[SPYN Record] Error checking pending sessions:', error);\n      }\n    };\n\n    // Sync all offline sessions\n    const syncAllOfflineSessions = async () => {\n      try {\n        setCurrentAnalysis('Synchronisation en cours...');\n        const result = await offlineService.default.syncPendingSessions(token || '');\n        if (result.synced > 0) {\n          Alert.default.alert('SuccÃ¨s', `${result.synced} session(s) synchronisÃ©e(s) !`);\n        }\n        if (result.failed > 0) {\n          Alert.default.alert('Attention', `${result.failed} session(s) ont Ã©chouÃ©. RÃ©essayez plus tard.`);\n        }\n\n        // Update pending count\n        const pendingCount = await offlineService.default.getPendingCount();\n        setPendingSyncCount(p